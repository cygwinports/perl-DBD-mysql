  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/DBD/mysql/.packlist
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/DBD/mysql/libmysql.dll.a
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/DBD/mysql/mysql.bs
  /usr/lib/perl5/vendor_perl/5.8/cygwin/auto/DBD/mysql/mysql.dll
  /usr/share/doc/Cygwin/perl-DBD-mysql-4.003.README
  /usr/share/doc/perl-DBD-mysql-4.003/ChangeLog
  /usr/share/doc/perl-DBD-mysql-4.003/README
  /usr/share/doc/perl-DBD-mysql-4.003/TODO
  /usr/share/man/man3/Bundle.DBD.mysql.3pm.gz
